{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Application Template",
    "keywords": ["yii2", "framework", "advanced", "application template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
	"repositories": [
	    {
	        "type": "package",
	        "package": {
	            "name": "gauthify",
	            "version": "dev-master",
	            "source": {
	                "url": "git@github.com:GAuthify/GAuthify-PHP.git",
	                "type": "git",
	                "reference": "master"
	            }
	        }
	    },
	    {
	        "type": "vcs",
	        "url": "git@github.com:cornernote/yii2-adldap-module.git"
	        }


	],

	"support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "dev-master",
        "yiisoft/yii2-swiftmailer": "*",
        "kartik-v/yii2-widgets": "*",
        "amnah/yii2-user": "2.1.0-RC",
        "warrence/yii2-kartikgii": "dev-master",
        "kartik-v/yii2-mpdf": "@dev",
        "sammaye/yii2-audittrail": "dev-master",
        "exertis/yii2-save-with-audit-trail-behavior": "dev-master",
        "kartik-v/yii2-detail-view": "dev-master",
        "2amigos/yii2-taggable-behavior": "*",
        "kartik-v/yii2-editable": "@dev",
        "kartik-v/yii2-krajee-base": "@dev",
        "kartik-v/yii2-password": "dev-master",
        "kartik-v/strength-meter": "1.1.0.*",
        "gauthify": "dev-master",
        "kartik-v/yii2-tabs-x": "@dev",
        "dmstr/yii2-db": "~0.1",
        "cebe/markdown": "1.0.1",
        "phpoffice/phpexcel": "dev-master",
        "knplabs/knp-snappy": "^0.3.0@dev",
        "cornernote/yii2-adldap-module": "dev-master",
        "atlasmobile/yii2-ga": "*",
        "cybercog/yii2-google-analytics": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
